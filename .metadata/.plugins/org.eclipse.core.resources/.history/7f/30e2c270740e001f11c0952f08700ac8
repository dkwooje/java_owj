		System.out.println(3 << 2); // 3*2*2
		System.out.println(-3 << 2);  //-3*2*2
		System.out.println();
		
		System.out.println(5 >> 1);  // 5/2 
		System.out.println(-5 >> 1); // -5/2 -1
		System.out.println(5 >> 2);  // 5/2/2 
